{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "es2019",
    "lib": [
      "ES2019"
    ],
    "module": "CommonJS",
    "declaration": true,
    "declarationMap": false,
    "declarationDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src"
  }
}
